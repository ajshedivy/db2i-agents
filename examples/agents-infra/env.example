# Db2 for i Agents env config

# Database connection settings (Mapepire)
HOST=your_host_name
DB_USER=your_username
PASSWORD=your_password
DB_PORT=8076  # default
SCHEMA=SAMPLE
READONLY=TRUE

# AI provider API keys (uncomment and add your keys as needed)

# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# IBM_WATSONX_API_KEY=
# IBM_WATSONX_PROJECT_ID=
# IBM_WATSONX_BASE_URL=
# IBM_WATSONX_MODEL_ID=

# PostgreSQL Database for AgentOS (optional, defaults provided)
POSTGRES_DB=ai
POSTGRES_USER=ai
POSTGRES_PASSWORD=ai
POSTGRES_SERVER=db
POSTGRES_PORT=5432
